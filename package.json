    "copy-to-clipboard": "^3.3.1",
    "embark": "^5.2.3",
    "embark-geth": "^5.2.3",
    "embark-ipfs": "^5.2.3",
    "embark-profiler": "^5.2.3",
    "embarkjs": "^5.2.3",
